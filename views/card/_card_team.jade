mixin team(teams)
	.team
		.title 队伍信息
			#addteam.func +
		.tabs
			.tab-titles
				each team in teams
					- var name = team.name;
					- var rel = team.rel;
					span.tab-title(tab-rel=rel) #{name}
			each team in teams
				.tab
					- var name = team.name;
					- var info = team.info;
					+tab(name, info)
		.addbox
			新建队伍啦啦啦

mixin tab(name, info)
	.line
		.left 队伍名
		.right #{name}
	.grid
		each item in info
			-var src = item.img;
			-var level = item.level;
			a.member
				img(src=src)
				span 等级#{level}

